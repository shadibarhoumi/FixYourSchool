<!DOCTYPE html>
<html>
<head>
  <title>Fixyourschool</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <link href='http://fonts.googleapis.com/css?family=Roboto+Slab:400,300|Carrois+Gothic+SC' rel='stylesheet' type='text/css'>
</head>
<body>
		<%= render 'layouts/header' %>
		<div id="content">
			<div id="flash-messages">
	<% flash.each do |name, msg| %>
		<%= content_tag :div, msg, id: "flash_#{name}" %>
	<% end %>
</div>
			<%= yield %></div>
		<%= render 'layouts/footer' %>
	</div>
</body>
</html>
